<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/sharing_description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sharing_attendants"
        android:layout_alignStart="@+id/sharing_attendants"
        android:layout_below="@+id/sharing_attendants"
        android:layout_marginStart="16dp"
        android:text="description"
        app:layout_constraintBaseline_toBaselineOf="@+id/check_description"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/check_description"
        android:layout_width="27dp"
        android:layout_height="33dp"
        android:layout_alignBaseline="@+id/sharing_description"
        android:layout_alignBottom="@+id/sharing_description"
        android:layout_alignLeft="@+id/check_attendants"
        android:layout_alignStart="@+id/check_attendants"
        android:layout_marginEnd="16dp"
        android:text="check_description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_attendants" />

    <TextView
        android:id="@+id/sharing_attendants"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sharing_location"
        android:layout_alignStart="@+id/sharing_location"
        android:layout_below="@+id/sharing_location"
        android:layout_marginStart="16dp"
        android:text="attendants"
        app:layout_constraintBaseline_toBaselineOf="@+id/check_attendants"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/check_attendants"
        android:layout_width="27dp"
        android:layout_height="33dp"
        android:layout_alignBaseline="@+id/sharing_attendants"
        android:layout_alignBottom="@+id/sharing_attendants"
        android:layout_alignLeft="@+id/check_location"
        android:layout_alignStart="@+id/check_location"
        android:layout_marginEnd="16dp"
        android:text="check_attendants"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_location" />

    <TextView
        android:id="@+id/sharing_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sharing_organizer"
        android:layout_alignStart="@+id/sharing_organizer"
        android:layout_below="@+id/sharing_organizer"
        android:layout_marginStart="16dp"
        android:text="location"
        app:layout_constraintBaseline_toBaselineOf="@+id/check_location"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/check_location"
        android:layout_width="27dp"
        android:layout_height="33dp"
        android:layout_alignBaseline="@+id/sharing_location"
        android:layout_alignBottom="@+id/sharing_location"
        android:layout_alignLeft="@+id/check_organizer"
        android:layout_alignStart="@+id/check_organizer"
        android:layout_marginEnd="16dp"
        android:text="check_location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_organizer" />

    <TextView
        android:id="@+id/sharing_organizer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sharing_title"
        android:layout_alignStart="@+id/sharing_title"
        android:layout_below="@+id/sharing_title"
        android:layout_marginStart="16dp"
        android:text="organizer"
        app:layout_constraintBaseline_toBaselineOf="@+id/check_organizer"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/check_organizer"
        android:layout_width="27dp"
        android:layout_height="33dp"
        android:layout_alignLeft="@+id/check_title"
        android:layout_alignStart="@+id/check_title"
        android:layout_below="@+id/check_title"
        android:layout_marginEnd="16dp"
        android:text="check_organizer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_title" />

    <TextView
        android:id="@+id/sharing_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/calendarname"
        android:layout_alignStart="@+id/calendarname"
        android:layout_below="@+id/calendarname"
        android:layout_marginStart="16dp"
        android:text="title"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="60dp" />

    <CheckBox
        android:id="@+id/check_title"
        android:layout_width="27dp"
        android:layout_height="33dp"
        android:layout_alignBottom="@+id/sharing_title"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="60dp"
        android:text="check_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/calendarname"
        android:layout_width="276dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="14dp"
        android:text="calendarname"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>